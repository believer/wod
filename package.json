{
  "name": "wod",
  "version": "1.37.0",
  "description": "A collection of CrossFit WODs",
  "scripts": {
    "build": "rescript build",
    "start": "cross-env BS_WATCH_CLEAR=true rescript build -w",
    "clean": "rescript clean",
    "test": "is-ci \"test:ci\" \"test:watch\"",
    "test:ci": "jest",
    "test:watch": "jest --watch",
    "server": "vite",
    "build:production": "vite build",
    "serve": "vite serve",
    "vercel-build": "npm run build && npm run build:production"
  },
  "keywords": [
    "CrossFit",
    "Reason",
    "WOD",
    "BuckleScript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@rescript/react": "0.10.3",
    "bs-css-emotion": "^1.2.0",
    "cuid": "2.1.8",
    "dedent": "0.7.0",
    "js-cookie": "2.2.1",
    "localforage": "1.8.1",
    "marked": "1.1.1",
    "pluralize": "8.0.0",
    "re-classnames": "5.0.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-html-parser": "2.0.2",
    "reason-react-update": "2.0.0"
  },
  "devDependencies": {
    "@glennsl/bs-jest": "0.5.1",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/release-notes-generator": "9.0.1",
    "@vitejs/plugin-react-refresh": "1.3.6",
    "autoprefixer": "9.8.5",
    "babel-jest": "26.1.0",
    "babel-polyfill": "6.26.0",
    "cross-env": "7.0.2",
    "is-ci-cli": "2.1.2",
    "rescript": "9.1.4",
    "tailwindcss": "1.5.2",
    "vite": "2.6.14"
  }
