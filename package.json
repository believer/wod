    "localforage": "1.7.3",
    "marked": "0.7.0",
    "pluralize": "8.0.0",
    "re-classnames": "4.0.0",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "reason-react": "0.7.0",
    "reason-react-update": "0.1.1",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@glennsl/bs-jest": "0.4.9",
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/commit-analyzer": "6.3.0",
